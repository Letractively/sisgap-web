	<queryString><![CDATA[SELECT
     --COUNT(*)
     SISGAP_FACTURA."STR_TIPODOC" AS SISGAP_FACTURA_STR_TIPODOC,
     SISGAP_FACTURA."NUM_NRODOC" AS SISGAP_FACTURA_NUM_NRODOC,
     SISGAP_SOCIO."TRAN_RAZON_SOCIAL" AS SISGAP_SOCIO_TRAN_RAZON_SOCIAL,
     SISGAP_FACTURA."DAT_FECHACRED" AS SISGAP_FACTURA_DAT_FECHACRED,
     SISGAP_FACTURA."NUM_TOTAL" AS SISGAP_FACTURA_NUM_TOTAL,
     SISGAP_FACTURA."NUM_ESTADO" AS SISGAP_FACTURA_NUM_ESTADO
     
     --SISGAP_DETALLEFACTURA."NUM_ACUENTA" AS SISGAP_DETALLEFACTURA_NUM_ACUE,
     --(SISGAP_FACTURA."NUM_TOTAL"-SISGAP_DETALLEFACTURA."NUM_ACUENTA")AS SISGAP_PENDIENTE,
     --SISGAP_DETALLEFACTURA."COD_ITEMCOBRANZA" AS SISGAP_DETALLEFACTURA_COD_ITEM
FROM
     "SISGAP"."SISGAP_SOCIO" SISGAP_SOCIO LEFT JOIN "SISGAP"."SISGAP_FACTURA" SISGAP_FACTURA ON SISGAP_SOCIO."TRAN_IDE" = SISGAP_FACTURA."TRAN_IDE"
     --INNER JOIN "SISGAP"."SISGAP_DETALLEFACTURA" SISGAP_DETALLEFACTURA ON SISGAP_FACTURA."COD_FACTURA" = SISGAP_DETALLEFACTURA."COD_FACTURA"
WHERE
     SISGAP_SOCIO."TRAN_IDE" NOT IN (SELECT DISTINCT SISGAP_FACTURA."TRAN_IDE" FROM "SISGAP"."SISGAP_FACTURA")
ORDER BY
     SISGAP_SOCIO."TRAN_RAZON_SOCIAL" ASC]]></queryString>

