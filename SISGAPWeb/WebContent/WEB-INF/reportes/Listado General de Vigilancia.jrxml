	<queryString><![CDATA[SELECT
     --COUNT(*) AS UNO
     SISGAP_SISAS_TMP."NRO" AS SISGAP_SISAS_TMP_NRO,
     TRIM(PADRON_MMH."NOMBRES") AS PADRON_MMH_NOMBRES,
     PADRON_MMH."GIROCOMER" AS PADRON_MMH_GIROCOMER,
     PADRON_MMH."SECTOR" AS PADRON_MMH_SECTOR,
     --TRIM(SISGAP_SOCIO."TRAN_CODIGO") AS SISGAP_SOCIO_TRAN_CODIGO,
     /* 22218  14 X 1587 */
     TO_CHAR(SISGAP_SISAS_TMP."PERIODO",'DD/MM/YYYY') AS SISGAP_SISAS_TMP_PERIODO,
     SISGAP_SISAS_TMP."DIAS" AS SISGAP_SISAS_TMP_DIAS,
     SISGAP_SISAS_TMP."PEND" AS SISGAP_SISAS_TMP_PEND,
     SISGAP_SISAS_TMP."TOTAL" AS SISGAP_SISAS_TMP_TOTAL
FROM
     "SISGAP"."SISGAP_SISAS_TMP" SISGAP_SISAS_TMP 
     INNER JOIN "SISGAP"."SISGAP_SOCIO" SISGAP_SOCIO ON TRIM(SISGAP_SISAS_TMP."NOMBRES") = TRIM(SISGAP_SOCIO."TRAN_CODIGO")
     INNER JOIN "SISGAP"."PADRON_MMH" PADRON_MMH ON TO_NUMBER(TRIM(SUBSTR(SISGAP_SOCIO."TRAN_CODIGO",3))) =  TO_NUMBER(TRIM(PADRON_MMH."PADRON"))
ORDER BY PADRON_MMH."SECTOR", SISGAP_SOCIO."TRAN_CODIGO", SISGAP_SISAS_TMP."PERIODO"]]></queryString>

